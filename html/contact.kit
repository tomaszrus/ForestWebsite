<!DOCTYPE html>
<html lang="pl">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <!-- @include 'styles.kit' -->
    <title>Forest Group</title>
</head>


<body class="contact-page">
    <!-- @include 'nav.kit' -->
    <!-- @include 'header.kit' -->

    <main>
        <section class="contact">
            <div class="wrapper">
                <form class="contact__form">
                    <div class="contact__form-box">

                        <label for="name" class="contact__form-label"><i class="fa-regular fa-id-card"></i> Imię i
                            nazwisko:</label>
                        <input type="text" class="contact__form-input" id="name"
                            placeholder="Podaj swoje imię i nazwisko">
                        <p class="contact__form-error-text">error</p>

                    </div>
                    <div class="contact__form-box">
                        <label for="email" class="contact__form-label"> <i class="fa-solid fa-at"></i> Adres
                            e-mail:</label>
                        <input type="email" class="contact__form-input" id="email" placeholder="Podaj adres email">
                        <p class="contact__form-error-text">error</p>
                    </div>

                    <div class="contact__form-box">
                        <label for="msg" class="contact__form-label"><i class="fa-solid fa-envelope"></i>
                            Wiadomość:</label>
                        <textarea id="msg" class="contact__form-textarea"
                            placeholder="Wpisz treść wiadomości"></textarea>
                        <p class="contact__form-error-text">error</p>
                    </div>
                    <button type="submit" class="send contact__form-btn btn-special-animation"> <i
                            class="fa-regular fa-paper-plane"></i> Wyślij</button>
                </form>
                <div class="google-maps">
                    <iframe
                        src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d2559.8462340662327!2d19.892022776880548!3d50.08916571331087!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x47165a326d015c3b%3A0x47a12cd367a1f34e!2sRestauracja%20McDonald&#39;s!5e0!3m2!1spl!2spl!4v1685607604682!5m2!1spl!2spl"
                        width="600" height="450" style="border:0;" allowfullscreen="" loading="lazy"
                        referrerpolicy="no-referrer-when-downgrade"></iframe>
                </div>
            </div>
        </section>
        <section class="map">
            <div class="wrapper">

            </div>
        </section>
    </main>
    <!-- @include 'footer.kit' -->
    <!-- @include 'scripts.kit' -->
    <script src="dist/js/validator.min.js"></script>
</body>

</html>